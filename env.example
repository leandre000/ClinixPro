# ClinixPro Environment Configuration
# Copy this file to .env.local and update the values

# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:8080
NEXT_PUBLIC_APP_NAME=ClinixPro
NEXT_PUBLIC_APP_VERSION=1.0.0

# Development Configuration
NODE_ENV=development

# Security Configuration
NEXT_PUBLIC_JWT_SECRET=clinixpro-frontend-jwt-secret-2024
NEXT_PUBLIC_ENCRYPTION_KEY=clinixpro-encryption-key-2024

# Feature Flags
NEXT_PUBLIC_ENABLE_ANALYTICS=false
NEXT_PUBLIC_ENABLE_DEBUG_MODE=true
NEXT_PUBLIC_ENABLE_MOCK_DATA=false

# External Services (if needed)
NEXT_PUBLIC_GOOGLE_ANALYTICS_ID=
NEXT_PUBLIC_SENTRY_DSN=

# Build Configuration
NEXT_PUBLIC_BUILD_TIME=2024-01-01T00:00:00Z
NEXT_PUBLIC_GIT_COMMIT_SHA= 